{
  "name": "cloudinary-metadata-api",
  "version": "1.0.0",
  "description": "A simple api for interacting with Cloudinary photo metadata.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "author": "nick.prozorovsky@gmail.com",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf node_modules",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prettier": "prettier 'src/**/*.{js,ts}' --config './.prettierrc.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm run build"
    }
  },
  "dependencies": {
    "axios": "^1.6.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.2.2"
  }
}
